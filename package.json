{
    "name": "ckeplugin-public-transport-picker",
    "version": "0.0.2",
    "description": "CKEditor public transport picker plugin for AM",
    "main": "index.js",
    "keywords": [
        "test"
    ],
    "wcmModule": {
        "frontend": {
            "dirPath": "public/app/",
            "moduleConfig": {
                "translationKey": "ckeplugin-public-transport-picker",
                "navigationItem": false,
                "authentication": {
                    "requiresLogin": false
