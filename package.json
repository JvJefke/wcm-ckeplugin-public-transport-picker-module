    "license": "ISC",
    "devDependencies": {
        "@district01/eslint-config": "~1.0.0",
        "gulp": "~3.9.1",
        "gulp-angular-templatecache": "~2.0.0",
        "gulp-clean": "~0.3.2",
        "gulp-replace": "~0.5.4",
        "gulp-zip": "~3.2.0",
        "run-sequence": "~1.2.2"
    }
}
